	"tokenColors": [
		{
			"scope": "string.json.comments",			// Extremely optional
			"settings": {													// Extremely optional
				"foreground": "#54aad8",					 // Extremely optional
			}
		},
		{
			"scope": [
				"constant.numeric.dec.python",		// Default numbers
				"constant.language.python",				// True / False / None
				"keyword.operator.assignment.python", // Equal char (=)
				"constant.numeric.hex.python", 				// 0x->02<-
				"storage.type.number.python",			// Numbers?
				"keyword.operator.arithmetic.python",	// Math chars ( + / - etc. )
				"keyword.operator.logical.python",			// not and or

			],
			"settings": {
				"foreground": "#89BEFA"	// Misc ()
			}
		},
		{
			"scope": [
				"comment",

			],
			"settings": {
				"foreground": "#8D949D", // comment
			}
		},
		{
			"scope": "string.quoted.docstring",
			"settings": {
				"foreground": "#AFD5FB"				// DocString
			}
		},
		{
			"scope": [
				"meta.function.decorator.python",

			],
			"settings": {
				"foreground": "#CBAAF9" // Decorators
			}
		},
		{
			"scope": [
				"keyword.control.flow.python",			// async, for, if elif else, etc.
				"storage.type.function.async.python", // async keyword before function declaration ( ->async<- def main() )
				"storage.type.function.python",				// 'def' keyword
				// "constant.language.python",						// True / False / None
				"storage.modifier.declaration.python",// 'global', etc.
				"storage.type.class.python",					// 'class' keyword
				"keyword.control.import.python",			// 'from' and 'import' keywords

			],
			"settings": {
				"foreground": "#EE8277" // Keywords
			}
		},
		{
			"scope": "keyword.other.unit",
			"settings": {
				"foreground": "#B5CEA8" //  Other
			}
		},
		{
			"scope": [
				"source.python",					// Variables and other
				"punctuation.definition.arguments.begin.python", // First square/round Parenthesis after method ( function(<-) )
				"punctuation.definition.arguments.end.python",		 // Last square/round Parenthesis after method ( dictionary[->] )
				"meta.function-call.arguments.python",						// Method arguments ( function(->Variable<-) )
				"meta.item-access.python",											// Var item access ( dictionary[->Key<-] )
				"meta.indexed-name.python",											// Indexed item access ( list[->Key<-] )
				"punctuation.definition.list.begin.python",				// List first scope ( List[<-])
				"punctuation.definition.list.end.python",					// List last scope ( List[->])
				"variable.parameter.function.language.python",			// Keyword parameter ( function(->param<-=True) )
				"punctuation.parenthesis.begin.python",						// Range first paren ( range = ->( ) )
				"punctuation.parenthesis.end.python",						// Range last paren ( range = ( ->) )

			],
			"settings": {
				"foreground": "#E7EDF2" // Default text
			}
		},
		{
			"scope": [
				"string.quoted.single.python",		// Default string in quotes '' or ""
				"string.quoted.multi.python",			// Triple-Quote String
				"storage.type.string.python",			// f or any other letter before string
				"constant.character.escape.python", // \n,\r etc.
				"string.regexp",										// String filtering, Commonly used with 're' module
				"support.type.property-name.json",
				
			],
			"settings": {
				"foreground": "#AFD5FB" // String
			}
		},
		{
			"scope": [
				"meta.function-call.generic.python",		// Function call
				"meta.function-call.python",						// Function call
				"support.function.builtin.python",			// Bultins like len(), print() etc.
				"entity.name.function.python",						// Function name ( def ->main<-() )
				"support.type.python",									// ->int<-(), ->list<-() etc.
				"meta.function.decorator.python",				// Class/Function decorators (->@staticmethod<-)
				"support.function.magic.python",					// Class functions ( def ->__init__<-() )

			],
			"settings": {
				"foreground": "#CBAAF9" // Functions / classes names and methods
			}
		},
		{
			"scope": [
				"entity.name.type.class",
				"entity.name.type.class.python",

				],		// Classes
			"settings": {
				"foreground": "#F2AA65"
			}
		}
	]
}
